package mdl.aula1;

public enum DireitaOuEsquerda {
	
	DIREITA(1, "D"),
	ESQUERDA(2, "E"),
	PARADO(3, "P");
	
	private Integer codigo;
	private String descricao;
	
	
	DireitaOuEsquerda(Integer codigo, String descricao){
		this.codigo = codigo;
		this.descricao = descricao;
	}
	
	public String getDescricao() {
		return descricao;
	}
	
	public Integer getCodigo() {
		return codigo;
	}

}
